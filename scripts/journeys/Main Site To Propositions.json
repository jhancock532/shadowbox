{
  "title": "Main Site To Propositions",
  "steps": [
    {
      "type": "setViewport",
      "width": 1440,
      "height": 864,
      "deviceScaleFactor": 1,
      "isMobile": false,
      "hasTouch": false,
      "isLandscape": false
    },
    {
      "type": "navigate",
      "url": "https://torchbox.com/",
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://torchbox.com/",
          "title": "Torchbox | Digital Agency - Charity, Nonprofit & Education Sectors"
        }
      ]
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [
        [
          "#main-content > div > div.home__hero > div:nth-child(3) > nav > ul > li:nth-child(1) > a > span"
        ],
        [
          "xpath///*[@id=\"main-content\"]/div/div[1]/div[3]/nav/ul/li[1]/a/span"
        ]
      ],
      "offsetY": 11.5,
      "offsetX": 151.6015625,
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://torchbox.com/digital-products/",
          "title": ""
        }
      ]
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [
        [
          "#our_clients > div > div > div > div > ul > li:nth-child(2) > div > a > img"
        ],
        ["xpath///*[@id=\"our_clients\"]/div/div/div/div/ul/li[2]/div/a/img"]
      ],
      "offsetY": 94.203125,
      "offsetX": 69.703125,
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://torchbox.com/work/pwa-digital-product-mozilla/",
          "title": ""
        }
      ]
    }
  ]
}
